{
    "name": "network-leaderboard",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "npx serve src",
        "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" && prettier --check \"src/**/*.{js,html,css}\" \"test/**/*.js\"",
        "lint:fix": "prettier --write \"src/**/*.{js,html,css}\" \"test/**/*.js\"",
        "test:types": "tsc --noEmit",
        "test:unit": "node --test",
        "test": "npm run test:types && npm run test:unit"
    },
    "devDependencies": {
        "@types/node": "^22.13.10",
        "eslint": "^8.42.0",
        "prettier": "^2.8.8",
        "typescript": "^5.3.0"
    }
}
